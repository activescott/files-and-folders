{
  "name": "@activescott/putty",
  "version": "1.0.0",
  "description": "Some putty to fill in the holes I've found in node.",
  "type": "module",
  "//exports": "https://nodejs.org/api/packages.html#main",
  "exports": {
    ".": "./dist/es/index.js",
    "./streams": "./dist/es/streams/index.js",
    "./fs": "./dist/es/fs.js"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "author": {
    "name": "Scott Willeke",
    "email": "scott@willeke.com",
    "url": "https://scott.willeke.com"
  },
  "license": "MIT",
  "keywords": [
    "fs",
    "filesystem"
  ],
  "repository": "github:activescott/files-and-folders",
  "homepage": "https://github.com/activescott/files-and-folders/blob/main/packages/@activescott/putty/README.md",
  "bugs": {
    "url": "https://github.com/activescott/files-and-folders/issues"
  },
  "dependencies": {
    "commander": "^9.3.0",
    "irritable-iterable": "^1.3.1"
  },
  "devDependencies": {
    "@activescott/eslint-config": "^2.3.5",
    "@tsconfig/node16-strictest-esm": "^1.0.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "build": "rm -rfd ./dist && ./node_modules/.bin/tsc --project tsconfig.json",
    "buildt": "rm -rfd ./dist && ./node_modules/.bin/tsc --project tsconfig.json --traceResolution",
    "eslint": "./node_modules/.bin/eslint . --ext ts,tsx,js,jsx",
    "lint": "./node_modules/.bin/prettier -l \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint",
    "lint-fix": "./node_modules/.bin/prettier --write \"{,!(node_modules)/**/}*.{ts,tsx,md,yml,json,html}\" && npm run eslint --fix",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "te": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prete": "npm run build",
    "pretest": "npm run lint && npm run build",
    "watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "dev": "npm run watch",
    "coverage-publish": "cat ./coverage/lcov.info | coveralls",
    "Xprepare": "cd ../.. && husky install packages/dedupe-files/.husky"
  }
}
